{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${version}",

  "name": "${mod_name}",
  "description": "Random worldgen things for minecraft.",
  "authors": [
    "Fluffy Bumblebee [Owner]",
    "IIpho3nix [Contributor-Artist]"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/terrains",
